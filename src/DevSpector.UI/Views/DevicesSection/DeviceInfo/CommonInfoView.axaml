<UserControl xmlns="https://github.com/avaloniaui"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:vm="using:DevSpector.Desktop.UI.ViewModels"
	xmlns:aura="using:Aura.UI.Controls"
	xmlns:l="clr-namespace:DevSpector.Desktop.UI.Localization"
	x:Class="DevSpector.Desktop.UI.Views.CommonInfoView">
	<StackPanel>
		<TextBlock Text="{x:Static l:Messages.InventoryNumber}" Classes="note" />
		<TextBlock Text="{Binding InventoryNumber}" Classes="main" />
		<TextBlock Text="{x:Static l:Messages.Type}" Classes="note" />
		<TextBlock Text="{Binding Type}" Classes="main" />
	</StackPanel>
</UserControl>
