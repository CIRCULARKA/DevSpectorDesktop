<UserControl xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:aura="using:Aura.UI.Controls"
    x:Class="DevSpector.Desktop.UI.Views.SettingsView" >
    <StackPanel VerticalAlignment="Center">
        <Button Content="" />
        <TextBlock Text="" Classes="note" />
        <TextBlock Text="{Binding InventoryNumber}" Classes="main" />
        <!-- <TextBlock Text="{x:Static l:Messages.Type}" Classes="note" /> -->
        <TextBlock Text="Тип" Classes="note" />
        <TextBlock Text="{Binding Type}" Classes="main" />
    </StackPanel>
</UserControl>
