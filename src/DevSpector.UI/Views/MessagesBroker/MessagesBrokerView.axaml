<UserControl xmlns="https://github.com/avaloniaui"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:shared="using:DevSpector.Desktop.UI.Views.Shared"
    xmlns:materialstyles="clr-namespace:Material.Styles;assembly=Material.Styles"
	x:Class="DevSpector.Desktop.UI.Views.MessagesBrokerView">
	<materialstyles:Card>
		<Grid ColumnDefinitions="*">
			<TextBlock Text="{Binding LoggedUserGroup}"
				Foreground="Gray" FontSize="11"
				HorizontalAlignment="Center" VerticalAlignment="Center" />
		</Grid>
	</materialstyles:Card>
</UserControl>
